{
  "manifest_version": 2,
  "name": "m-tab",
  "version": "0.1.0",
  "description": "新标签页美化插件，material风格，好看的背景图片，炫酷的背景切换效果。",
  "icons": {
    "16": "logo/16.png",
    "32": "logo/32.png",
    "48": "logo/48.png",
    "64": "logo/64.png",
    "96": "logo/96.png",
    "128": "logo/128.png"
  },
  "page_action": {
    "default_title": "新标签",
    "default_icon": "logo/64.png"
  },
  "author": "程刁",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "tabs"
  ]
}
