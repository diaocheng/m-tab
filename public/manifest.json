{
  "manifest_version": 2,
  "name": "nsf-tab",
  "version": "0.0.1",
  "description": "新标签页美化插件，material风格，包含书签，浏览记录和好看的背景页",
  "icons": {
    "16": "icon/ico16.png",
    "48": "icon/ico48.png",
    "96": "icon/ico96.png",
    "128": "icon/ico128.png"
  },
  "page_action": {
    "default_title": "新标签",
    "default_icon": "icon/ico48.png",
    "default_popup": "popup.html"
  },
  "author": "程刁",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "management",
    "bookmarks",
    "tabs",
    "history",
    "unlimitedStorage",
    "http://*/",
    "https://*/"
  ]
}
